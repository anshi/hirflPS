record(DigitalScanPS, "$(AREA):$(PS_SN):$(FUNC)")
{
	field(DESC, "Send command to scan ps device")
	field(IP_ADDR, "$(IP)")
	field(PORT, "$(PORT)")
	field(DEV_CHAN, "01")
	field(DTYP, "Digital Scan PS INTR")
	field(PREC, "4")
	field(LCOMM, "$(FUNC)")
	field(SCAN, "I/O Intr")
	field(OUT, "$(PARM)")
	field(OUT1, "$(OUT1) PP")
}
