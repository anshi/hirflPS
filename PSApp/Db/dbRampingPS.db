record(ps, "$(AREA):$(PS_SN)")
{
	field(DESC, "PS")
	field(PORT, "$(DATA_PORT)")
	field(DOMAIN, "$(DATA_IP)")
	field(DTYP, "PSSoft")
	field(SCAN, "I/O Intr")
	field(PREC, "4")
	field(NELM, "160000")
	field(FTVL, "FLOAT")
	field(DTYPE, "$(DTYPE)")
	field(INP, "$(AREA):$(PS_SN):AI.STATE")
	field(OUT, "$(AREA):$(PS_SN):AI.COMMAND")
}
record(DigitalDCPSao, "$(AREA):$(PS_SN):AI")
{
	field(DESC, "Send command to ps device")
	field(IP_ADDR, "$(IP)")
	field(PORT, "$(PORT)")
	field(DEV_CHAN, "01")
	field(DTYP, "Digital Ramping PS")
	field(PREC, "4")
	field(LCOMM, "AI")
	field(SCAN, "I/O Intr")
	field(OUT, "@$(PARM)")
	field(OUT1, "$(AREA):$(PS_SN):error.INFORMCODE PP")
}
record(Informin, "$(AREA):$(PS_SN):error")
{
	field(DESC, "Error message of PS")
	field(DTYP, "Ramping PS Inform")
	field(SCAN, "Passive")
}
